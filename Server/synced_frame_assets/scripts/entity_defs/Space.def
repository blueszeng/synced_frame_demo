<root>
	<Volatile>
		<position/>
		<yaw/>
		<pitch> 20 </pitch>
	</Volatile>

	<Interfaces>
		<Interface>			GameObject			</Interface>
	</Interfaces>

	<Properties>

		<spaceKey>
			<Type>			SPACE_KEY				</Type>
			<Flags>			BASE				</Flags>
			<Persistent>	true				</Persistent>
		</spaceKey>

		<spaceKeyC>
			<Type>			SPACE_KEY				</Type>
			<Flags>			CELL_PUBLIC				</Flags>
			<Persistent>	true				</Persistent>
		</spaceKeyC>

		<spaceState>
			<Type>			INT8				</Type>
			<Flags>			CELL_PUBLIC				</Flags>
			<Default>		-1			    	</Default>
			<Persistent>	true				</Persistent>
		</spaceState>

		<spaceFarmeId>
			<Type>			FRAMEID			</Type>
			<Flags>			CELL_PUBLIC			</Flags>
		</spaceFarmeId>

		<framePool>
			<Type>			FRAME_LIST			</Type>
			<Flags>			CELL_PUBLIC			</Flags>
		</framePool>
		
	</Properties>

	<BaseMethods>
		<loginToSpace>
			<Arg>		ENTITYCALL		</Arg>
		</loginToSpace>

		<logoutSpace>
			<Arg>		ENTITY_ID		</Arg>
		</logoutSpace>

		<onEnter>
			<Arg>		ENTITYCALL		</Arg>
		</onEnter>

		<onLeave>
			<Arg>		ENTITY_ID		</Arg>
		</onLeave>

		<onStateChange>
			<Arg>		INT8		</Arg>
		</onStateChange>	
	</BaseMethods>

	<CellMethods>
		<onEnter>
			<Arg>		ENTITYCALL		</Arg>
		</onEnter>

		<onLeave>
			<Arg>		ENTITY_ID		</Arg>
		</onLeave>

		<onReady>
			<Arg>		ENTITYCALL		</Arg>
			<Arg>		INT8		</Arg>
		</onReady>

		<onGamePause>
		</onGamePause>

		<onGameRunning>
		</onGameRunning>

	</CellMethods>
</root>

